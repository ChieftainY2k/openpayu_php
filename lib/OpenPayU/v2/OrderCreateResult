class OpenPayU_OrderCreateResult extends OpenPayU_Result
{
    private $continueUrl = '';
    private $refReqId = '';
    private $reqId = '';
    private $version = '';
    private $properties = '';
    private $products = '';
    private $buyer = '';
    private $fee = '';
    private $totalAmount = '';
    private $validityTime = '';
    private $customerIp = '';
    private $orderUrl = '';
    private $notifyUrl = '';
    private $merchantPosId = '';
    private $payMethods = '';
    private $extOrderId = '';
    private $settings = '';
    private $currencyCode = '';
    private $description = '';
    private $shippingMethodsUrl = '';
    private $shippingMethods = '';
    private $completeUrl = '';
    private $cancelUrl = '';
    private $shippingMethod = '';

    /**
     * @param mixed $buyer
     */
    public function setBuyer($buyer)
    {
        $this->buyer = $buyer;
    }

    /**
     * @return mixed
     */
    public function getBuyer()
    {
        return $this->buyer;
    }

    /**
     * @param mixed $cancelUrl
     */
    public function setCancelUrl($cancelUrl)
    {
        $this->cancelUrl = $cancelUrl;
    }

    /**
     * @return mixed
     */
    public function getCancelUrl()
    {
        return $this->cancelUrl;
    }

    /**
     * @param mixed $completeUrl
     */
    public function setCompleteUrl($completeUrl)
    {
        $this->completeUrl = $completeUrl;
    }

    /**
     * @return mixed
     */
    public function getCompleteUrl()
    {
        return $this->completeUrl;
    }

    /**
     * @param mixed $continueUrl
     */
    public function setContinueUrl($continueUrl)
    {
        $this->continueUrl = $continueUrl;
    }

    /**
     * @return mixed
     */
    public function getContinueUrl()
    {
        return $this->continueUrl;
    }

    /**
     * @param mixed $currencyCode
     */
    public function setCurrencyCode($currencyCode)
    {
        $this->currencyCode = $currencyCode;
    }

    /**
     * @return mixed
     */
    public function getCurrencyCode()
    {
        return $this->currencyCode;
    }

    /**
     * @param mixed $customerIp
     */
    public function setCustomerIp($customerIp)
    {
        $this->customerIp = $customerIp;
    }

    /**
     * @return mixed
     */
    public function getCustomerIp()
    {
        return $this->customerIp;
    }

    /**
     * @param mixed $description
     */
    public function setDescription($description)
    {
        $this->description = $description;
    }

    /**
     * @return mixed
     */
    public function getDescription()
    {
        return $this->description;
    }

    /**
     * @param mixed $extOrderId
     */
    public function setExtOrderId($extOrderId)
    {
        $this->extOrderId = $extOrderId;
    }

    /**
     * @return mixed
     */
    public function getExtOrderId()
    {
        return $this->extOrderId;
    }

    /**
     * @param mixed $fee
     */
    public function setFee($fee)
    {
        $this->fee = $fee;
    }

    /**
     * @return mixed
     */
    public function getFee()
    {
        return $this->fee;
    }

    /**
     * @param mixed $merchantPosId
     */
    public function setMerchantPosId($merchantPosId)
    {
        $this->merchantPosId = $merchantPosId;
    }

    /**
     * @return mixed
     */
    public function getMerchantPosId()
    {
        return $this->merchantPosId;
    }

    /**
     * @param mixed $notifyUrl
     */
    public function setNotifyUrl($notifyUrl)
    {
        $this->notifyUrl = $notifyUrl;
    }

    /**
     * @return mixed
     */
    public function getNotifyUrl()
    {
        return $this->notifyUrl;
    }

    /**
     * @param mixed $orderUrl
     */
    public function setOrderUrl($orderUrl)
    {
        $this->orderUrl = $orderUrl;
    }

    /**
     * @return mixed
     */
    public function getOrderUrl()
    {
        return $this->orderUrl;
    }

    /**
     * @param mixed $payMethods
     */
    public function setPayMethods($payMethods)
    {
        $this->payMethods = $payMethods;
    }

    /**
     * @return mixed
     */
    public function getPayMethods()
    {
        return $this->payMethods;
    }

    /**
     * @param mixed $products
     */
    public function setProducts($products)
    {
        $this->products = $products;
    }

    /**
     * @return mixed
     */
    public function getProducts()
    {
        return $this->products;
    }

    /**
     * @param mixed $properties
     */
    public function setProperties($properties)
    {
        $this->properties = $properties;
    }

    /**
     * @return mixed
     */
    public function getProperties()
    {
        return $this->properties;
    }

    /**
     * @param mixed $refReqId
     */
    public function setRefReqId($refReqId)
    {
        $this->refReqId = $refReqId;
    }

    /**
     * @return mixed
     */
    public function getRefReqId()
    {
        return $this->refReqId;
    }

    /**
     * @param mixed $reqId
     */
    public function setReqId($reqId)
    {
        $this->reqId = $reqId;
    }

    /**
     * @return mixed
     */
    public function getReqId()
    {
        return $this->reqId;
    }

    /**
     * @param mixed $settings
     */
    public function setSettings($settings)
    {
        $this->settings = $settings;
    }

    /**
     * @return mixed
     */
    public function getSettings()
    {
        return $this->settings;
    }

    /**
     * @param mixed $shippingMethod
     */
    public function setShippingMethod($shippingMethod)
    {
        $this->shippingMethod = $shippingMethod;
    }

    /**
     * @return mixed
     */
    public function getShippingMethod()
    {
        return $this->shippingMethod;
    }

    /**
     * @param mixed $shippingMethods
     */
    public function setShippingMethods($shippingMethods)
    {
        $this->shippingMethods = $shippingMethods;
    }

    /**
     * @return mixed
     */
    public function getShippingMethods()
    {
        return $this->shippingMethods;
    }

    /**
     * @param mixed $shippingMethodsUrl
     */
    public function setShippingMethodsUrl($shippingMethodsUrl)
    {
        $this->shippingMethodsUrl = $shippingMethodsUrl;
    }

    /**
     * @return mixed
     */
    public function getShippingMethodsUrl()
    {
        return $this->shippingMethodsUrl;
    }

    /**
     * @param mixed $totalAmount
     */
    public function setTotalAmount($totalAmount)
    {
        $this->totalAmount = $totalAmount;
    }

    /**
     * @return mixed
     */
    public function getTotalAmount()
    {
        return $this->totalAmount;
    }

    /**
     * @param mixed $validityTime
     */
    public function setValidityTime($validityTime)
    {
        $this->validityTime = $validityTime;
    }

    /**
     * @return mixed
     */
    public function getValidityTime()
    {
        return $this->validityTime;
    }

    /**
     * @param mixed $version
     */
    public function setVersion($version)
    {
        $this->version = $version;
    }

    /**
     * @return mixed
     */
    public function getVersion()
    {
        return $this->version;
    }

}